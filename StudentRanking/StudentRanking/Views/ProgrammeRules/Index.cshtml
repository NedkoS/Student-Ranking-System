@model IEnumerable<StudentRanking.Models.ProgrammeProperties>

<style>
    table, th, td {
        border: 0px solid black;
        border-collapse: collapse;
    }

    th, td {
        padding: 15px;
    }
</style>

@section Scripts
{
<script type="text/javascript" src="~/JavaScript/ProgrammeRules.js">
</script>
}

@{
    ViewBag.Title = "Index";
    var programmes = ViewBag.Programmes as Dictionary<String, List<String>>;
}

<div><h4>Изберете факултет</h4></div>
@Html.DropDownList("faculty", ViewData["faculties"] as SelectList, new { @id = "faculty" })
<select id="programmeName"></select>
<input type="submit" value="Покажи" onclick="javascript:ChooseProgramme()" />

<br>

<div id="resultTable">
    @Html.Partial("_ProgrammePropertiesTable", Model)
</div>